{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.17.1",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --port 3001 --host",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "typecheck": "vue-tsc --noEmit -p tsconfig.json",
    "prepare": "husky"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.68",
    "@iconify-json/simple-icons": "^1.2.53",
    "@nuxt/content": "3.7.1",
    "@nuxt/image": "1.11.0",
    "@nuxt/ui": "4.0.0",
    "@unhead/vue": "^2.0.17",
    "@vueuse/nuxt": "13.9.0",
    "better-sqlite3": "^12.4.1",
    "motion-v": "^1.7.2",
    "nuxt-og-image": "5.1.11",
    "unstorage": "^1.17.1",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.0.8"
  },
  "devDependencies": {
    "@hrcd/eslint-config": "^3.0.3",
    "@nuxtjs/i18n": "10.1.0",
    "eslint": "^9.36.0",
    "husky": "^9.1.7",
    "nuxt": "^4.1.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "sharp"
    ],
    "ignoredBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "vue-demi"
    ]
  }
}
